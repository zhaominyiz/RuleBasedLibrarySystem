<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="cse.sa.rulebasedsystem.Entities.BookrecordEntity">
        <table name="bookrecord" schema="library" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="bookId">
                <column name="book_id" nullable="false" precision="11"/>
            </basic>
            <basic name="date">
                <column name="date" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
